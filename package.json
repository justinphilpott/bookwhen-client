{
  "name": "@jphil/bookwhen-client",
  "version": "0.2.6",
  "description": "TypeScript client for the Bookwhen API (v2)",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepare": "pnpm husky install",
    "dev": "vitest",
    "preview": "vite preview --config vite.preview.config.ts",
    "build": "vite build -c vite.library.config.ts",
    "build:node": "vite build -c vite.test.config.ts",
    "lint": "tsc",
    "test": "vitest run",
    "test:browser": "playwright test",
    "test:node": "vitest run",
    "coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "prettier-check": "prettier --check 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' README.md",
    "pre-commit": "pnpm lint && pnpm build",
    "commit": "cz",
    "prepush": "pnpm test",
    "local-release": "changeset version && changeset publish",
    "release": "pnpm lint && pnpm test && pnpm build && changeset publish",
    "ci": "pnpm test && pnpm check-exports && pnpm build"
  },
  "keywords": [
    "bookwhen",
    "booking",
    "api client"
  ],
  "type": "module",
  "author": {
    "name": "Justin Philpott",
    "email": "justin@jphil.dev"
  },
  "homepage": "https://github.com/justinphilpott/bookwhen-client",
  "bugs": {
    "url": "https://github.com/justinphilpott/bookwhen-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justinphilpott/bookwhen-client.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "license": "ISC",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@mswjs/data": "^0.16.2",
    "@mswjs/http-middleware": "^0.10.3",
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitest/coverage-v8": "2.1.3",
    "c8": "^10.1.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.6",
    "msw": "^2.7.5",
    "playwright": "^1.52.0",
    "playwright-msw": "^3.0.1",
    "prettier": "^3.3.3",
    "tsup": "^8.3.4",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^5.1.0",
    "vitest": "^2.1.3",
    "axios": "^1.7.7",
    "zod": "^3.23.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "axios": "^1.7.0",
    "zod": "^3.23.0"
  },
  "dependencies": {},
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.es.js",
      "node": "./dist/index.js",
      "import": "./dist/index.es.js",
      "default": "./dist/index.es.js"
    }
  },
  "browser": "./dist/index.es.js"
}
